module Data.Function;

import Stdlib.Prelude open;

uncurry
  : {A : Type}
    -> {B : Type}
    -> {C : Type}
    -> (A -> B -> C)
    -> A × B
    -> C
  | f (a, b) := f a b;

curry
  : {A : Type}
    -> {B : Type}
    -> {C : Type}
    -> (A × B -> C)
    -> A
    -> B
    -> C
  | f a b := f (a, b);
